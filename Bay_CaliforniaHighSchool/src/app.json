{
  "expo": {
    "name": "fbla-overseer",
    "description": "Submission for the 2020 FBLA Mobile Development competition. The application was built by Taylor Allen and Om Chaudhary.",
    "slug": "fbla-overseer",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "bundleIdentifier": "com.taylorallen.fblaoverseer-ios",
      "supportsTablet": true
    },
    "android": {
      "package": "com.taylorallen.fblaoverseer",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
